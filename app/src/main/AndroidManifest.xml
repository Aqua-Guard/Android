<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo_"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AquaGuard"
        tools:targetApi="31">
        <activity
            android:name=".SplashScreen1"
            android:exported="false" />
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"></activity>

        <activity android:name=".MainActivity"/>
        <activity android:name=".SplashScreen2"></activity>
        <activity android:name=".SplashScreen3"></activity>
        <activity android:name=".SplashScreen4"></activity>
    </application>

</manifest>